#Download Dracula theme
wget https://raw.githubusercontent.com/dracula/vim/master/colors/dracula.vim -P $HOME/.config/nvim/colors/
wget https://raw.githubusercontent.com/morhetz/gruvbox/master/colors/gruvbox.vim -P $HOME/.config/nvim/colors/
wget https://raw.githubusercontent.com/chriskempson/base16-vim/master/colors/base16-dracula.vim -P $HOME/.config/nvim/colors/
wget https://raw.githubusercontent.com/junegunn/seoul256.vim/master/colors/seoul256.vim -P $HOME/.config/nvim/colors/
